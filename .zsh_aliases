# constants
export DEVPATH="$HOME/dev"
export D_EDITOR="cursor"

# commands
alias gen-pass="openssl rand -base64"
alias ip="ifconfig | grep broadcast | cut -d ' ' -f2"
alias pubip="curl -s https://checkip.amazonaws.com"
alias pserver="ip && python3 -m http.server --directory"
alias ll="eza --long --icons --header --git -a"
alias tree="eza --tree"

# git aliases
alias gpl="git pull"
alias gps="git push"
alias gcd="git checkout develop"
alias gs="git status -s"
alias gb="git branch"
alias gcb="git cb"
